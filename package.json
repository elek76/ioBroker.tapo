  "devDependencies": {
    "@iobroker/testing": "^4.1.0",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/json-bigint": "^1.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/proxyquire": "^1.3.31",
